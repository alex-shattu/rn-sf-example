{
  "name": "FirstReact",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "run-script-os",
    "start:win32": "npm run reverse && npx react-native start",
    "start:linux:darwin": "npx react-native start",
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "pods": "cd ./ios && rm -rf Pods && pod install && cd ..",
    "test": "jest",
    "reinstall": "run-script-os",
    "reinstall:win32": "rmdir /q/s node_modules && npm i",
    "reinstall:linux:darwin": "rm -rf node_modules && npm i",
    "cache": "npm clean cache --force && yarn clean cache",
    "reverse": "adb reverse tcp:8081 tcp:8081"
  },
  "sdkDependencies": {
    "SalesforceMobileSDK-Android": "https://github.com/forcedotcom/SalesforceMobileSDK-Android.git#v8.1.0",
